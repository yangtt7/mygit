@charset "utf-8";
/* 禁用iPhone中Safari的字号自动调整 */
html {-webkit-text-size-adjust: 100%;-ms-text-size-adjust: 100%;}
/* 去除iPhone中默认的input样式 */
input{ -webkit-appearance:none;  resize: none;}
/* 取消链接高亮  */
body,div,ul,li,ol,input,textarea,select,p,dl,dt,dd,a,img,button,form, footer,header,nav,section{ -webkit-tap-highlight-color:rgba(0, 0, 0, 0);
    font-family: Microsoft YaHei;}
/* 设置HTML5元素为块 */
article, aside, details,figcaption,figure,footer,header,hgroup, menu,nav,section {
display: block;}
/* 图片自适应 */
img {
    max-width: 100%;
    height: auto;
    width:auto\9; /* ie8 */
    -ms-interpolation-mode:bicubic;/*为了照顾ie图片缩放失真*/
}
*:focus {outline:none;}
/* 清零 */
body,div,ul,li,ol,input,textarea,select,p,dl,dt,dd,a,img,button,form,footer,header,nav,section{margin:0; padding:0; border:none;}
body{width: 100%;height: 100%;font-family:Tahoma,Arial,Roboto,"Droid Sans","Helvetica Neue","Droid Sans Fallback","Heiti SC",sans-self; color:#999;min-width: 320px;font-family: Helvetica,sans-serif;}

em,i{font-style:normal;}
.clearfix:after{content:""; display:block; visibility:hidden; height:0; clear:both;}
.clearfix{zoom:1;}
a{text-decoration:none; font-family:Tahoma,Arial,Roboto,"Droid Sans","Helvetica Neue","Droid Sans Fallback","Heiti SC",sans-self;}
a:hover{text-decoration:none;}
ul,ol,li{list-style:none;}
fieldset,img{border: none;}
table {border-collapse:collapse;border-spacing:0;}
caption, th {text-align:left;}
q:before,q:after {content:"";}
