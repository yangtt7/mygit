header .logo{width: 16.312rem;height:2.5rem;background:url(../images/logo.png) no-repeat;left:4%;top:3%;}
 #mob_1{width: 100%;height: 18.75rem;top:30%;overflow:hidden;left:0;top:35%;text-align: center;} 
@-webkit-keyframes run{
    0%{left:0;opacity: 0.5;}
    100%{left:20%;}
}
.rz{width:5.625rem;height:2.75rem;background: url(../images/rz.png) no-repeat;left:0;top:18%;z-index: 10;animation:run 0.5s;-webkit-animation:run 0.5s linear 0s alternate;-webkit-animation-fill-mode:forwards;}
@-webkit-keyframes lt{
    0%{left:8%;opacity: 0;}
    100%{left:32%;}
}
.lt{width:9.812rem;height:4.812rem;background: url(../images/lt.png) no-repeat;left:8%;top:15%;z-index: 9;animation:lt 0.8s;-webkit-animation:lt 1s linear 0s  alternate;-webkit-animation-fill-mode:forwards;}
@-webkit-keyframes ting_pic{
    0%{left:21%;opacity:0.5;}
    100%{left:52%;}
}
.ting_pic{width: 2.062rem;height: 1.688rem;background: url(../images/ting_pic.png) no-repeat;left:21%; top:13%;animation:ting_pic 1.2s;-webkit-animation:ting_pic 0.8s linear 0s  alternate;-webkit-animation-fill-mode:forwards;}
.dh{width: 2.062rem;height: 1.375em;background: url(../images/dh.png) no-repeat;left:52%; top:21%;}
@-webkit-keyframes jj{
    0%{left:21%;opacity:0.5;}
    100%{left:32%;}
}
.jj{width: 7.437em;height: 1.812em;background: url(../images/jj.png) no-repeat;left:22%; top:24%;animation:jj 1.2s;-webkit-animation:jj 1.2s linear 0s  alternate;-webkit-animation-fill-mode:forwards;}
.fy{width: 19.562rem;height: 1.813rem;background: url(../images/fy.png) no-repeat;left:22%; top:28%;animation:jj 1.2s;-webkit-animation:jj 1.2s linear 0s  alternate;-webkit-animation-fill-mode:forwards;}
@-webkit-keyframes shang{
   from{left:0%;}
   to{left:-20%;}
}
@-webkit-keyframes center{
   from{left:0%;}
   to{left:10%;}
}
.shang{width: 75rem;height: 18.75rem;background: url(../images/shang.png) no-repeat;left:0; top:18%;animation:shang 5s;-webkit-animation:shang 5s infinite linear 1.2s  alternate;} 
.center{width: 75rem;height: 18.75rem;background: url(../images/center.png) no-repeat; top:19%;animation:center 4s;-webkit-animation:center 4s infinite linear 1.2s  alternate;}
.xia{width: 75rem;height: 11.125rem;background: url(../images/xia.png) no-repeat;left:0; top:40%;animation:shang 3s;-webkit-animation:shang 3s infinite linear 1.2s  alternate;}
.buy_b{width: 28.125rem;height: 6.25rem;background: url(../images/buy_m.png) no-repeat;left:50%; top:70%;margin-left: -14rem;}
.arrow{width: 3.375rem;height: 1.563rem;background: url(../images/arrow.png) no-repeat;left:50%; top:90%;margin-left:-1.687rem;}
.buy_win{width: 28.125rem;height:30rem;border-radius:1.438rem;border:1px solid  red;box-shadow: 0px 2px 10px 1px red;left:50%;top:25%;margin-left: -14rem;z-index:15;background: #fff;}
.buy_top{width:12.938rem;height: 1.813em;background: url(../images/buy_top.png) no-repeat;left:50%; top:14%;margin-left:-6.8rem;}
.buy_win ul{left:50%;margin-left:-9.3rem;top:30%;}
.buy_win ul li{font-size:30px;width: 18.5rem;height: 4rem;border:1px solid #565b5c;line-height: 4rem;text-align: center;margin-bottom:1.6rem;color:#565b5c;}
.buy_m{width: 21.25rem;height: 6.25rem;background: url(../images/buy_b.png) no-repeat;bottom:10%;left:50%;margin-left:-10.8rem;}
.close_btn{width:4.375rem;height: 4.375rem;border:1px solid red;border-radius:50%; background:#fff;right:-8%;top:-5%;box-shadow: 0px 2px 10px 1px red;}
.close_btn p:nth-child(1){width: 0.125rem;height:3.3rem;background:red;-webkit-transform:rotate(45deg);top:9%;left:47%;}
.close_btn p:nth-child(2){width: 0.125rem;height:3.3rem;background:red;-webkit-transform:rotate(-45deg);top:9%;left:47%;}

/* 第二页 */
@-webkit-keyframes first{
   from{left:0%;opacity:0.5;}
   to{left:19%;}
}
.first{width: 11.813rem;height: 1.812rem;background: url(../images/first.png) no-repeat;top:12%;animation:first 0.8s;-webkit-animation:first 0.8s  linear 0s alternate;-webkit-animation-fill-mode:forwards;}
@-webkit-keyframes time{
   from{left:0%;opacity:0.5;}
   to{left:27%;}
}
.time{width: 13.063rem;height: 1.813rem;background:url(../images/time.png) no-repeat;top:16%;animation:time 1s;-webkit-animation:time 1s  linear 0s alternate;-webkit-animation-fill-mode:forwards;}

.quick{width: 16.5rem;height: 4.875rem;background:url(../images/quick.png) no-repeat;left:0;top: 21%;animation:quick 1s;-webkit-animation:quick 1s  linear 2s alternate;-webkit-animation-fill-mode:forwards;}
.tin{animation:tin 1s;-webkit-animation:tin 1s  linear 2s alternate;-webkit-animation-fill-mode:forwards;}
.tin,.tin_1,.tin_2{width: 2.063rem;height: 1.688rem;background:url(../images/ting_pic.png) no-repeat;left:35%;top: 21%;}
.map_con{width: 28.813rem;height: 25.313rem;background:url(../images/map.png) no-repeat;left: 50%;top: 37%;margin-left: -16rem;}

.lx{width: 3rem;height: 1.438em;background:url(../images/lx.png) no-repeat;right: 28%;animation:lx 1s;top:45%;-webkit-animation:lx 1s  linear 0s alternate;-webkit-animation-fill-mode:forwards;}

.dw{width: 2.215rem;height: 2.688rem;background:url(../images/dw.png) no-repeat;right: 28%;top:25%;z-index:10;animation:dw 1s;-webkit-animation:dw 1s  linear 0s alternate;-webkit-animation-fill-mode:forwards;}
.x{width: 22.188rem;height: 18.875rem;background:url(../images/x.png) no-repeat; left: 12%;top: 9%;z-index:8;}
.dd{width: 22.188rem;height: 18.875rem;background:url(../images/d.png) no-repeat;left: 12%;top: 9%;}
.map_con ul li:nth-child(1){width:9.375rem;height:4rem;background:url(../images/qg.png) no-repeat;right: 45%;top: 65%;}
.arrow_2{width: 3.375rem;height: 1.563rem;background:url(../images/arrow.png) no-repeat;left:50%;top:85%;margin-left:-1.6rem;}
/*第三页*/
.confident{width: 9.378rem;height: 1.813rem;background:url(../images/confident.png) no-repeat;left:12%;top:15%;}
.yz{width: 7.438rem;height: 2.188rem;background:url(../images/yz.png) no-repeat;left:20%;top:20%;}
.num_one{width: 11.813rem;height: 4.813rem;background:url(../images/num_one.png) no-repeat;left:36%;top:16%;}
.tin_1{left:46%;top:15%;}
.pc{width: 16.813rem;height: 1.813rem;background:url(../images/pc.png) no-repeat;left:14%;top:26%;}
.round{width: 20.5rem;height: 20.5rem;background:url(../images/round.png) no-repeat;left:50%;top:37%;margin-left:-10rem;}
.one{width: 5.75rem;height: 1.938rem;background:url(../images/one.png) no-repeat; left:50%;top:47%;margin-left:-2.8rem;}
.round ul li:nth-child(1){width: 9.875rem;height: 4rem;background:url(../images/year.png) no-repeat; right: -11%;top: 15%;}
.round ul li:nth-child(2){width:13.125rem;height:4rem;background:url(../images/jinyin.png) no-repeat;right: -36%;top: 61%;}
.round ul li:nth-child(3){width:7.375rem;height:4rem;background:url(../images/dd.png) no-repeat;right: 33%;top: 95%;}
.round ul li:nth-child(4){width: 13.063rem;height:4rem;background:url(../images/hj.png) no-repeat;right: 73%;top: 57%;}
.round ul li:nth-child(5){width: 10rem;height:4rem;background:url(../images/world.png) no-repeat;right: 80%;top: 15%;}
/*第四页*/
.sd{width: 8.063rem;height: 1.813rem;background:url(../images/sd.png) no-repeat;left:12%;top:16%;}
.jx{width: 14.25rem;height: 4.75rem;background:url(../images/jx.png) no-repeat;left:30%;top:13%;}
.tin_2{left:51%;top:13%;}
.yd{width: 8.063rem;height: 1.813rem;background:url(../images/yd.png) no-repeat;left:21%;top:23%;}
.wn{width: 8.313rem;height: 1.813rem;background:url(../images/wn.png) no-repeat;left:30%;top:27%;}
.show{width: 22.875rem;height: 10.188rem;background:url(../images/show.png) no-repeat;left:50%;top:58%;margin-left:-14rem;z-index:12;}
.zw{width: 12.75rem;height: 17.188rem;background:url(../images/zw.png) no-repeat;left:50%;top:40%;margin-left:-8.8rem;z-index: 5;}
.buy_4{width: 28.125rem;height: 6.25rem;background:url(../images/buy_m.png) no-repeat;left:50%;top:72%;margin-left:-16rem;z-index:15;}
@media screen and (min-width:320px) and (max-width:568px){
    header .logo{background-size:70% 70%;}
