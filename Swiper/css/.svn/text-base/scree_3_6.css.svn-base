    .buy_m{bottom:-9%;left:50%;margin-left:-6rem;}
    .close_btn{width: 2.5rem;height: 2.5rem;border-radius:50%;border:1px solid red; background:#fff;right:-5%;top:-5%;box-shadow: 0px 2px 10px 1px red;}
    .close_btn p:nth-child(1){width: 0.125rem;height:2rem;background:red;-webkit-transform:rotate(45deg);top:9%;left:47%;}
    .close_btn p:nth-child(2){width: 0.125rem;height:2rem;background:red;-webkit-transform:rotate(-45deg);top:9%;left:47%;}
    .map_con{margin-left:-11rem;}
    .lx{right: 45%; top: 31%;}
    .dw{right: 47%;top: 21%;}
    .x,.dd{left: 8.5%;top: 6%;}
    .map_con ul li:nth-child(1){right:12%;top:9.6%;}
    .arrow_2{top:89%;}
    .num_one{left:42%;}
    .tin_1{left:57%;}
    .round{left:53%;}
    .one{left:39%;top:34%;}
    .round ul li:nth-child(1){left:-4%;}
    .round ul li:nth-child(2){left:-14%;top:47%;}
    .round ul li:nth-child(3){left:27%;top:65%;}
    .round ul li:nth-child(4){left:48%;top:49%;}
    .round ul li:nth-child(5){left:62%;top:16%;}
    .jx{left:37%;}
    .show{margin-left:-9rem;}
    .zw{margin-left:-5.4rem;}
    .buy_4{margin-left:-10rem;}
    .tin_2{left:65%;}
    #mob_1{width: 100%;height: 10.75rem;overflow:hidden;left:0;top:30%;text-align: center;} 

    @-webkit-keyframes shang{
   from{left:0%;}
   to{left:-20%;}
}
 @-webkit-keyframes center{
   from{left:0%;}
   to{left:10%;}
}
@-webkit-keyframes lt{
    0%{left:0;opacity: 0.5;}
    100%{left:36%;}
} 
@-webkit-keyframes ting_pic{
    0%{left:21%;opacity:0.5;}
    100%{left:63%;}
}
}

